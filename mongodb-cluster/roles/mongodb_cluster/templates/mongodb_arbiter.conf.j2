storage:
  dbPath: {{ base_data_dir }}/arbiter

systemLog:
  destination: file
  path: {{ log_dir }}/mongodb_arbiter.log
  logAppend: true

net:
  port: {{ arbiter_port }}
  bindIp: 0.0.0.0

processManagement:
  fork: false
  pidFilePath: /var/run/mongodb-arbiter/mongodb_arbiter.pid

replication:
  replSetName: {{ data_replset }}
